! special
*.foreground:   #eaeaea
*.background:   #161616
*.cursorColor:  #eaeaea

! black
*.color0:       #1c1d1f
*.color8:       #4e5053

! red
*.color1:       #ad2818
*.color9:       #dd4d3c

! green
*.color2:       #65a643
*.color10:      #85df56

! yellow
